{
  "name": "htmlSanityCheck.js",
  "description": "This module provides some basic sanity checks on html files",
  "version": "1.0.0",
  "main": "./lib/cli.js",
  "dependencies": {
    "debug": "4.3.4",
    "js-yaml": "4.1.0",
    "node-html-parser": "^6.1",
    "strip-json-comments": "3.1.1",
    "yargs": "16.2.0",
    "yargs-parser": "20.2.4",
    "yargs-unparser": "2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uniqueck/htmlSanityCheck.js.git"
  },
  "bugs": {
    "url": "https://github.com/uniqueck/htmlSanityiCheck.js/issues/"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "dirty-chai": "2.0.1",
    "mocha": "10.1.0",
    "rewiremock": "^3.14.3",
    "sinon": "^16",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-ideal": "^1.0.0"
  },
  "scripts": {
    "test": "mocha",
    "lint": "eslint \"./lib/**/*.js\" \"./test/**/*.js\"",
    "lint:fix": "eslint \"./lib/**/*.js\" \"./test/**/*.js\" --fix",
    "lfet:UI": "exec java -classpath \"$INIT_CWD/.lfet/tool/lib/*\" lohrfink.lfet.Application &",
    "lfet:install": "mkdir -p \"$INIT_CWD/.lfet/tool\" && cd \"$INIT_CWD/.lfet/tool\" && curl -fsSLO https://www.lohrfink.de/lfet.latest.inst && unzip -o lfet.latest.inst",
    "lfet:update": "cd \"$INIT_CWD/.lfet/tool/lib\" && curl -fsSLO https://www.lohrfink.de/lfet.latest.upd && unzip -o lfet.latest.upd",
    "lfet:GenSrc": "exec java -classpath \"$INIT_CWD/.lfet/tool/lib/*\" lohrfink.lfet.Application -gs \"$INIT_CWD/lfet/\" -Recursive -FileNamePattern \"*.lfet\" -SkipUnchanged -SourceCode \"JavaScript\"",
    "lfet:GenTest": "exec java -classpath \"$INIT_CWD/.lfet/tool/lib/*\" lohrfink.lfet.Application -GenTest -Directory \"$INIT_CWD/lfet/\" -Recursive -FileNamePattern \"*.lfet\" -intags \"cucumber\" -OutputRootfolder \"$INIT_CWD/test/features\" -ContinueOnError"
  },
  "authors": [
    "Constantin Kr√ºger (https://github.com/uniqueck)"
  ],
  "license": "MIT",
  "bin": {
    "htmlsanitycheck": "bin/htmlsanitycheck",
    "htmlsanitycheck.js": "bin/htmlsanitycheck"
  }
}
